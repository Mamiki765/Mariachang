{
  "name": "mariachang",
  "version": "3.6.0",
  "private": true,
  "author": "Rika Amamiya",
  "description": "Alter from MiniPotato Bot by EOi",
  "main": "main.mjs",
  "type": "module",
  "scripts": {
    "start": "node --trace-warnings main.mjs",
    "local_start": "node --env-file=.env.local --trace-warnings main.mjs",
    "local_debug": "node --env-file=.env.development --trace-warnings main.mjs",
    "_comment_format": "npm run format:check で整形が必要なファイルを確認し、npm run format で自動整形する",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "_comment_tree": "npm run treeでプロジェクトの構造を project-structure.txt に出力する",
    "tree": "tree --ignore \"node_modules,.git,.env,package-lock.json,project-structure.txt\" -l 3 -o project-structure.txt"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.0",
    "axios": "^1.9.0",
    "break_infinity.js": "^2.2.0",
    "cron-parser": "^5.3.1",
    "discord.js": "^14.23.2",
    "express": "^5.1.0",
    "image-size": "^2.0.2",
    "mee6-levels-api": "^1.3.1",
    "node-cron": "^4.2.1",
    "pg": "^8.16.3",
    "rss-parser": "^3.13.0",
    "sequelize": "^6.37.7",
    "simple-git": "^3.28.0"
  },
  "engines": {
    "node": "22.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mamiki765/Mariachang.git"
  },
  "license": "MIT",
  "keywords": [
    "discord",
    "discord.js",
    "bot",
    "lost-arcadia",
    "ロストアーカディア",
    "pbw",
    "koyeb",
    "node"
  ],
  "devDependencies": {
    "cross-env": "^10.0.0",
    "dotenv": "^17.2.2",
    "prettier": "^3.6.2",
    "tree-cli": "^0.6.7"
  }
}
