.mode csv
.headers on
.output icons.csv
SELECT * FROM icons;
.output points.csv
SELECT * FROM points;
.output currentdominos.csv
SELECT * FROM currentdominos;
.output dominohistorys.csv
SELECT * FROM dominohistorys;
.output admin_memos.csv
SELECT * FROM admin_memos;
.quit
.mode csv
.headers on
.output characters.csv
SELECT * FROM characters;
.exit
